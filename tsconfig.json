{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "types": [],
    "lib": ["ES2020"],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "paths": {
      "_helpers/*": ["_helpers/*"],
      "_middleware/*": ["_middleware/*"],
      "users/*": ["users/*"]
    }
    
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
